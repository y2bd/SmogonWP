<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.0">
  <CommandSet xml:lang="en-US" Name="BaseVCD">
    <CommandPrefix>Smogon</CommandPrefix>
    <Example> open the pokemon page </Example>

    <Command Name="OpenPokemon">
      <Example> open the Pokemon page  </Example>
      <ListenFor> open [the] pokemon [page] </ListenFor>
      <Feedback> Opening the Pokemon page... </Feedback>
      <Navigate Target="/View/PokemonSearchView.xaml"/>
    </Command>

    <Command Name="OpenMoves">
      <Example> open the moves page </Example>
      <ListenFor> open [the] moves [page] </ListenFor>
      <Feedback> Opening the Moves page... </Feedback>
      <Navigate Target="/View/MoveSearchView.xaml"/>
    </Command>

    <Command Name="OpenAbilities">
      <Example> open the abilities page  </Example>
      <ListenFor> open [the] abilities [page] </ListenFor>
      <Feedback> Opening the Abilities page... </Feedback>
      <Navigate Target="/View/AbilitySearchView.xaml"/>
    </Command>

    <Command Name="SearchPokemon">
      <Example> search for the pokemon Gardevoir</Example>
      <ListenFor> search [for] [the] pokemon {Pokemon} </ListenFor>
      <Feedback> Searching for {Pokemon}... </Feedback>
      <Navigate Target="/View/PokemonSearchView.xaml?pokemon"/>
    </Command>

